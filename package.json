    "eslint-plugin-promise": "^5.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-unicorn": "^20.0.0",
    "fast-glob": "3.2.4",
    "prettier": "2.4.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^18.6.5",
    "cross-env": "7.0.3",
    "np": "6.2.5",
    "rimraf": "3.0.2"
