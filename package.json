  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.json",
    "deps": "yarn upgrade-interactive --latest",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "prettier": "prettier -c --write \"**/*\"",
